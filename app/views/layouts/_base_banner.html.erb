<div class="max-width-5 mx-auto top-90 top-nav">
  <div class="clearfix">
    <div class="col col-6">&nbsp;</div>
    <div class="col col-2">&nbsp;</div>
    <div class="col col-1 login-links left-align">
      <%= link_to "Sign up!", register_path unless current_user %>
    </div>
    <div class="col col-1 login-links left-align">
      <%= link_to "Sign in", login_path unless current_user %>
    </div>
    <div class="col col-1 login-links left-align">
      <%= link_to "Sign in with Google", "/auth/google" unless current_user %>
    </div>
