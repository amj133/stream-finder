<%= render partial: 'layouts/banner_1' %>
<%= render partial: 'layouts/navbar' %>
<%= render("shared/validation_errors", :errors => @errors) %>

<div class="p2 bg-white border rounded my2 mx-auto sign-in edit">
  <%= form_for @project do |f| %>
    <h3>Create a new project: </h3>

    <div class="edit-entry">
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div><br>

    <div class="edit-entry">
      <%= f.label :huc, 'HUC' %>
      <%= f.text_field :huc %>
    </div><br>

    <div class="sign-in-button">
      <%= f.submit 'Create Project', class: 'register' %>
    </div>
  <% end %>
</div>
